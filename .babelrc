{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "commonjs",
        "targets": {
          "esmodules": false
        }
      }
    ],
    "@babel/preset-react",
    "@lingui/babel-preset-react"
  ],
  "plugins": [
    "macros",
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "regenerator": true,
        "useESModules": true
      }
    ]
  ]
}
